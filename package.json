{
    "name": "ng-table-builder",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build:app": "ng build --prod",
        "build:lib": "ts-node --project tools/tsconfig.tools.json tools/build && ts-node --project tools/tsconfig.tools.json ./tools/copy-readme",
        "format": "npx sort-package-json && prettier --write \"**/*.{js,ts,html,css,scss,md,json,yml}\"",
        "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
        "lint": "eslint \"**/*.ts\"",
        "start": "ng serve --open --aot=false --port 4300",
        "test": "jest --config ./jest.config.js"
    },
    "prettier": "@angular-ru/prettier-config",
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.26",
        "@angular-ru/eslint-config": "12.21.5",
        "@angular-ru/prettier-config": "12.19.1",
        "@angular/animations": "8.2.14",
        "@angular/cdk": "8.2.3",
        "@angular/cli": "8.3.26",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/compiler-cli": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@angular/material": "8.2.3",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "@types/jest": "24.0.13",
        "@types/node": "12.7.5",
        "acorn": "6.1.1",
        "colors": "1.3.3",
        "core-js": "2.6.9",
        "eslint": "6.8.0",
        "hammerjs": "2.0.8",
        "husky": "3.0.5",
        "intersection-observer": "0.7.0",
        "jest": "24.8.0",
        "jest-cli": "24.8.0",
        "jest-preset-angular": "7.1.0",
        "ng-packagr": "9.0.3",
        "ngx-toastr": "10.0.4",
        "pretty": "2.0.0",
        "rxjs": "6.5.2",
        "sort-package-json": "1.40.0",
        "ts-node": "7.0.0",
