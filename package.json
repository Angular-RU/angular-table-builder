{
    "name": "table-builder",
    "version": "0.0.0",
    "scripts": {
        "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
        "// - Binaries CI": "Run against /node_modules/.bin in Jenkins",
        "ng:ci": "/node_modules/@angular/cli/bin/ng",
        "jest:ci": "/node_modules/jest/bin/jest.js",
        "ng": "ng",
        "lint": "ng lint",
        "test": "jest --config ./jest.config.js --maxConcurrency=4",
        "test:ci": "yarn jest:ci --config ./jest.config.js --coverage --maxConcurrency=3",
        "start": "ng serve --open --aot=false --port 4300",
        "build": "yarn ng:ci build && yarn ng:ci build table-builder",
        "build:lib": "ng build table-builder && ts-node --project tsconfig.tools.json ./tools/copy-readme",
        "build.meta": "echo 'prebuild'",
        "format": "prettier --write \"src/**/*.*\" \"projects/**/*.*\" \"!src/assets/**\" \"!src/favicon.ico\""
    },
    "private": true,
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.5",
        "@angular-devkit/build-ng-packagr": "0.803.5",
        "@angular/animations": "8.2.7",
        "@angular/cdk": "8.2.0",
        "@angular/cli": "8.3.5",
        "@angular/common": "8.2.7",
        "@angular/compiler": "8.2.7",
        "@angular/compiler-cli": "8.2.7",
        "@angular/core": "8.2.7",
        "@angular/forms": "8.2.7",
        "@angular/language-service": "8.2.7",
        "@angular/material": "8.2.0",
        "@angular/platform-browser": "8.2.7",
        "@angular/platform-browser-dynamic": "8.2.7",
        "@angular/router": "8.2.7",
        "@types/jest": "24.0.13",
        "@types/node": "12.7.5",
        "acorn": "6.1.1",
        "codelyzer": "5.0.1",
        "colors": "1.3.3",
        "core-js": "2.6.9",
        "hammerjs": "2.0.8",
        "husky": "3.0.5",
        "intersection-observer": "0.7.0",
        "jest": "24.8.0",
        "jest-cli": "24.8.0",
        "jest-preset-angular": "7.1.0",
        "ng-packagr": "5.2.0",
        "ngx-toastr": "10.0.4",
        "prettier": "1.17.1",
        "pretty": "2.0.0",
        "protractor": "5.4.0",
        "rxjs": "6.5.2",
        "ts-node": "7.0.0",
        "tsickle": "0.37.0",
        "tslib": "1.9.0",
        "tslint": "5.20.0",
        "tslint-eslint-rules": "5.4.0",
        "tslint-microsoft-contrib": "5.2.1",
        "tslint-sonarts": "1.9.0",
        "typescript": "3.5.1",
        "zone.js": "0.10.2"
    }
}
